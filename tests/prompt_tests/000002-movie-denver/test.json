{
    "name": "Denver Movies Playing",
    "description": "Get a movie playing at a theater and save to a text file.",
    "yaml_prompt": "movie-denver.yaml",
    "output_files": [
        "000002-movie-denver.txt"
    ],
    "exec": {
        "command": "scripts/main.py",
        "arguments": [
            "--continuous",
            "--continuous-limit",
            "20",
            "--use-yaml-prompt"
        ],
        "env": {
            "TEMPERATURE": "0",
            "EXECUTE_LOCAL_COMMANDS": "True"
        }
    }
}