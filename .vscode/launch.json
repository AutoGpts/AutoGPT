    "version": "0.2.0",
    "configurations": [

        {
            "name": "R - Legacy cli",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/autogpts/autogpt/autogpt/app/cli.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/autogpts/autogpt/:${env:PYTHONPATH}"
            }
        },
        {

            "name": "R - New Cli",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/autogpts/autogpt/autogpt/core/runner/cli_app/cli.py",
            "args": ["run"],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/autogpts/autogpt/:${env:PYTHONPATH}"
            }
            
        }
    ]
}